def prime_mag(arr):
    rows_with_prime = []
    for row in arr:
        for number in row:
            factors = 0
            for i in range(number + 1):
                if number % (i + 1) == 0:
                    factors += 1
            if factors == 2:
                rows_with_prime.append(row.tolist())
                break
    print(rows_with_prime)

arr = np.array([
[2, 3, 5],
[14, 6, 8],
[11, 13, 17],
[7, 10, 13]
])

prime_mag(arr)


